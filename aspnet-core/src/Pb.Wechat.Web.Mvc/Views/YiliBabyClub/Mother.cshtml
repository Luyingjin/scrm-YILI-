    <environment names="Development">
        <link rel="stylesheet" href="~/view-resources/views/YiliBabyClub/mother/dist/mc/bundle.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/view-resources/views/YiliBabyClub/mother/dist/muying/bundle.css" asp-append-version="true" />
